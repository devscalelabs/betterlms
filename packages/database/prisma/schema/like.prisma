model PostLike {
  id String @id @default(cuid())

  userId String
  user User @relation(fields: [userId], references: [id])

  postId String
  post Post @relation(fields: [postId], references: [id])

  createdAt DateTime @default(now())

  @@unique([userId, postId])
  @@index([postId])
  @@index([userId])
}

